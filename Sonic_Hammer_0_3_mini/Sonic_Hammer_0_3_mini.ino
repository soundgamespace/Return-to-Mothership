    for (int i = 0; i < 3; i++) {
      digitalWrite(redPins[i], LOW);
      digitalWrite(greenPins[i], HIGH);
      digitalWrite(bluePins[i], HIGH);
    }
    startPlayback(redSample, sizeof(redSample));
    outColor = 1;
  }
  else if (g > 150 && outColor != 2 && g > r + b) {
    for (int i = 0; i < 3; i++) {
      Serial.println("GREEN");
      digitalWrite(redPins[i], HIGH);
      digitalWrite(greenPins[i], LOW);
      digitalWrite(bluePins[i], HIGH);
    }
    startPlayback(greenSample, sizeof(greenSample));
    outColor = 2;
  }
  else if (b > 150 && outColor != 3 && b > r + g) {
    for (int i = 0; i < 3; i++) {
      Serial.println("BLUE");
      digitalWrite(redPins[i], HIGH);
      digitalWrite(greenPins[i], HIGH);
      digitalWrite(bluePins[i], LOW);
    }
    startPlayback(blueSample, sizeof(blueSample));
    outColor = 3;
  }
  else if (outColor == 0) {
    Serial.println("WHITE");
    for (int i = 0; i < 3; i++) {
      analogWrite(redPins[i], HIGH);
      analogWrite(greenPins[i], HIGH);
      analogWrite(bluePins[i], HIGH);
    }
    outColor = -1;
  }
}

